non-fluents inventory_3nf {
	domain = inventory;

	objects{
		item: {i1, i2, i3};
	};
	
	non-fluents {
		MAX-ORDER = 8;
	};
}

instance inventory_2 {
	domain = inventory;
	non-fluents = inventory_3nf;

	init-state{
		stock(i1) = 0;
		stock(i2) = 0;
		stock(i3) = 0;
	};

	max-nondef-actions = pos-inf;
	horizon = 10;
	discount = 1.0;
}
