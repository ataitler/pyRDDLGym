non-fluents inventory_2nf {
	domain = inventory;

	objects{
		item: {i1, i2};
	};
	
	non-fluents {
		MAX-ORDER = 6;
	};
}

instance inventory_1 {
	domain = inventory;
	non-fluents = inventory_2nf;

	init-state{
		stock(i1) = 0;
		stock(i2) = 0;
	};

	max-nondef-actions = pos-inf;
	horizon = 10;
	discount = 1.0;
}
